
@{
    ViewBag.Title = "PrincipalCurator";
    Layout = "~/Views/Shared/_LayoutPrincipalCurator.cshtml";
}
@section Scripts
{

    <script src="@Url.Content("~/Scripts/globalConstants.js")"></script>
    <script src="@Url.Content("~/js/PrincipalCuratorView.js")"></script>
    <script src="@Url.Content("~/Scripts/plugins/typehead/bootstrap3-typeahead.min.js")"></script>
}
<div class="text-center m-t-lg">
    <h1>
        @ViewData["SubTitle"]
    </h1>
    <small>@ViewData["Message"]</small>
</div>
<div class="tabs-container">
    <ul class="nav nav-tabs">
        <li class="active"><a data-toggle="tab" href="#tab-unassigned" style="background-color:red; color:white;" aria-expanded="true">Unassigned Publications</a></li>
        <li class=""><a data-toggle="tab" href="#tab-assigned" style="background-color:#00B85F; color:white;" aria-expanded="false">Assigned Publications</a></li>
        <li class=""><a data-toggle="tab" href="#tab-history" style="background-color:black;  color:white;"aria-expanded="false">Publications History</a></li>
    </ul>
    <div class="tab-content">
        <div id="tab-unassigned" class="tab-pane active">
            <div class="panel-body">
                <div class="wrapper wrapper-content animated fadeInRight">
                    <div class="row">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="ibox float-e-margins">

                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="ibox float-e-margins">
                                    <div class="ibox-title">
                                        <h5>Listing Unassigned Publications</h5>
                                    </div>
                                    <div class="ibox-content">
                                        <div class="table-responsive">
                                            <table class="table table-striped table-bordered table-hover dataTables-example" id="unassigned-publications">
                                                <thead>
                                                    <tr>
                                                        <th>ID</th>
                                                        <th>Title</th>
                                                        <th>Description</th>
                                                        <th>KICD Number</th>
                                                        <th>Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="tab-assigned" class="tab-pane">
            <div class="panel-body">
                <div class="wrapper wrapper-content animated fadeInRight">
                    <div class="row">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="ibox float-e-margins">

                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="ibox float-e-margins">
                                    <div class="ibox-title">
                                        <h5>Listing Assigned Publications</h5>
                                    </div>
                                    <div class="ibox-content">
                                        <div class="table-responsive">
                                            <table class="table table-striped table-bordered table-hover dataTables-example" id="assigned-publications">
                                                <thead>
                                                    <tr>
                                                        <th>Title</th>
                                                        <th>Description</th>
                                                        <th>KICD Number</th>
                                                        <th>Price</th>
                                                        <th>Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="tab-history" class="tab-pane">
            <div class="panel-body">
                <div class="wrapper wrapper-content animated fadeInRight">
                    <div class="row">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="ibox float-e-margins">

                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="ibox float-e-margins">
                                    <div class="ibox-title">
                                        <h5>Listing Previous Publications</h5>
                                    </div>
                                    <div class="ibox-content">
                                        <div class="table-responsive">
                                            <table class="table table-striped table-bordered table-hover dataTables-example" id="history-publications">
                                                <thead>
                                                    <tr>
                                                        <th>Title</th>
                                                        <th>Description</th>
                                                        <th>KICD Number</th>
                                                        <th>Price</th>
                                                        <th>Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>
