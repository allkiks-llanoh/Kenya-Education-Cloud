@using KEC.Curation.UI.Models;
@model ChiefCurators
@{
    ViewBag.Title = "Publications to curate";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Scripts{
    @Scripts.Render("~/js/curatorToCurate")
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="row">
            <div class="col-lg-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h5>Listing Publications To Curate</h5>

                    </div>
                    <div class="ibox-content">

                        <div class="table-responsive">
                            <table class="table table-striped table-bordered table-hover dataTables-example"
                                   data-curateurl="@ViewBag.CurateUrl" id="publications-to-curate">
                                <thead>
                                    <tr>
                                        <th>Id</th>
                                        <th>Publication</th>
                                        <th>Section To Curate</th>
                                        <th>Assignment Date</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                  
                                </tbody>

                            </table>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="form-group col-md-10">
    <input type="text" name="CurrentUserGuid" id="CurrentUserGuid" class="form-control" value="@Model.Guid">
</div>