
@{
    ViewBag.Title = "ListLevels";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@{
    ViewBag.Title = "ListCategory";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Scripts
{
    <script src="@Url.Content("~/js/ListingLevels.js")"></script>
    <script src="~/Scripts/plugins/typehead/bootstrap3-typeahead.min.js"></script>
}
<div class="text-center m-t-lg">
    <h1>
        @ViewData["SubTitle"]
    </h1>
    <small>@ViewData["Message"]</small>
</div>
<div class="tabs-container">

    <div class="tab-content">
        <div id="tab-unassigned" class="tab-pane active">
            <div class="panel-body">
                <div class="wrapper wrapper-content animated fadeInRight">
                    <div class="row">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="ibox float-e-margins">
                                    <a href="@Url.Action("Levels", "Home")" class="btn btn-success pull-left">Create Level <i class="fa fa-plus"></i></a>

                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="ibox float-e-margins">
                                    <div class="ibox-title">
                                        <h5>Listing Categories</h5>
                                    </div>
                                    <div class="ibox-content">
                                        <div class="table-responsive">
                                            <table class="table table-striped table-bordered table-hover dataTables-example" id="LevelList">
                                                <thead>
                                                    <tr>
                                                        <th>ID</th>
                                                        <th>Name</th>
                                                        <th>Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

</div>


