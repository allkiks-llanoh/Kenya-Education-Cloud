@{
    ViewData["Title"] = "Home Page";
}
<br>
<script src="~/js/jquery-3.1.1.min.js"></script>
<script src="~/js/GetContent.js"></script>
<nav class="navbar navbar-default navbar-fixed-top"  style="height:70px;">
    <div class="container">
        <div class="navbar-header">
            <button type="button"  class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand">
                <img src="~/images/KEClogo.png" style="margin-top:-20px;" width="150" height="70" />
            </a>
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav"></ul>
            <partial name="_LoginPartial" />
        </div>
    </div>
</nav>
<br />
<div class="container-fluid">
    <div class="well">
        <div class="ibox-content">
            <div class="table-responsive">
                <table class="table table-striped table-bordered table-hover dataTables-example" id="unassigned-publications">
                    <thead>
                        <tr>
                            <th>Description</th>
                            <th>Book</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
    @{
        var email = User.Identity.Name;
    }
    <input class="hidden" type="text" name=userEmail id="userEmail" value="@email" />
</div>
